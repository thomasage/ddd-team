{
    "name": "thomasage/ddd-team",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpstan/phpstan": "^0.12.64",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Team\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
